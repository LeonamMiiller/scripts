# Generate SSH-Key
## 
# ssh-keygen -t rsa-sha2-512
##
Host ssh.dev.azure.com
  IdentityFile ~/.ssh/azure/user_name/id_rsa
  IdentitiesOnly yes
  HostkeyAlgorithms +ssh-rsa
  PubkeyAcceptedKeyTypes=rsa-sha2-512


Host github.com
  HostName github.com
  User git
  IdentityFile ~/.ssh/github/LeonamMiiller/id_rsa
